  <title>DMA Real Estate</title>

<link rel="DMA icon" type="image/png" href="https://homesppc-images.s3.us-east-2.amazonaws.com/DMA+Real+Estate+w+real+thumb.png"/>


  <%- include('_header') %>
  <h1 class="text-center" style="color:#3E3F3C;">DMA Real Estate</h1>
  
  
    <% articles.forEach(article => { %>
      <% if(article.page === "home"){ %>
    <div class="card mt-4">
      <div class="card-body">
        <h4 class="card-title"><%= article.title %></h4>
        <div class="card-subtitle text-muted mb-2">
          <%= article.createdAt.toLocaleDateString() %>
        </div>
        <div class="embed-responsive embed-responsive-16by9">
          <iframe class="embed-responsive-item" src= "<%= article.description %>" allowfullscreen></iframe>
        </div>
        
        
        <div class="accordion" id="accordionExample">
          <div class="card">
            <div class="card-header" id="headingOne">
              <h2 class="mb-0">
                <button class="btn btn-link btn-block text-left" type="button" data-toggle="collapse" data-target="#collapse<%= article.id %>" aria-expanded="true" aria-controls="collapseOne">
                  Video Description
                </button>
              </h2>
            </div>
        
            <div id="collapse<%= article.id %>" class="collapse" aria-labelledby="headingOne" data-parent="#accordionExample">
              <div class="card-body">
                <%= article.markdown %>
              </div>
            </div>
          </div>
       
      </div>

    </div>
    <% } %>
    <% }) %>
  
